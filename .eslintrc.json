{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "plugin:import/typescript"   
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "@typescript-eslint",
      "import"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "import/no-cycle": "off",
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": ["error"],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
